{
    "manifest_version": 2,

    "name": "Links4All",
    "description": "share links and content browser extension",
    "version": "0.1",

    "permissions": [
        "activeTab",
        "tabs",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": ["js/index.js"]
        }
    ]
}
